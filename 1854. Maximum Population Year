// Java Solution 

class Solution {
    public int maximumPopulation(int[][] logs) {
        int[] years = new int[101]; 
        for (int[] log : logs) {
            years[log[0] - 1950]++;     
            years[log[1] - 1950]--;      
        }
        int maxpop = 0, maxyear = 1950, currentpop = 0;
        for (int i = 0; i < years.length; i++) {
            currentpop += years[i];
            if (currentpop > maxpop) {
                maxpop = currentpop;
                maxyear = 1950 + i;
            }
        }
        return maxyear;
    }
}
