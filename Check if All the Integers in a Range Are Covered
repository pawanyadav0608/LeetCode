bool isCovered(int** ranges, int rangesSize, int* rangesColSize, int left, int right) {
   int pawan[52]={0};
    for(int i=0 ; i<rangesSize ; i++){
        int start = ranges[i][0];
        int end = ranges[i][1];
        pawan[start]++ ;
        pawan[end + 1]-- ;
    }

    for(int i=1 ; i<52 ; i++){
        pawan[i]=pawan[i]+pawan[i-1];

    }

    for(int i=left ; i<=right ;i++){
        if(pawan[i]<=0){
            return false;
        }
    }
return true;
} 
