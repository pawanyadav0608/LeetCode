class Solution {
    public int numPairsDivisibleBy60(int[] time) {
        int[] rems = new int[60];
        int count = 0;
        for (int t : time) {
            int rem = t % 60;
            int com = (60 - rem) % 60;
            count += rems[com];
            rems[rem]++;
        }
        return count;
    }
}
